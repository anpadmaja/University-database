      <operation name="removeCourse" qname="operNS:removeCourse" xmlns:operNS="http://Services" returnQName="retNS:removeCourseReturn" xmlns:retNS="http://Services" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
