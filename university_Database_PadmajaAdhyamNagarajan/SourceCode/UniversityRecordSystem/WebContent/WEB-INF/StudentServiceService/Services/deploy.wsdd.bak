      <operation name="getInstructorDetails" qname="operNS:getInstructorDetails" xmlns:operNS="http://Services" returnQName="retNS:getInstructorDetailsReturn" xmlns:retNS="http://Services" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
