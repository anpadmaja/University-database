      <operation name="searchCourseDetails" qname="operNS:searchCourseDetails" xmlns:operNS="http://Services" returnQName="retNS:searchCourseDetailsReturn" xmlns:retNS="http://Services" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
